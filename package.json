{
    "name": "calendar_management",
    "version": "1.0.0",
    "description": "A calendar management app",
    "main": "index.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:coverage": "jest --coverage",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up",
        "docker:down": "docker-compose down",
        "db:create": "cd migrations && docker build -t db_mero .",
        "db:start": "cd migrations && docker run -d --name db_mero -e POSTGRES_PASSWORD=password -p 5432:5432 postgres",
        "db:delete": "docker stop db_mero && docker rm db_mero"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@prisma/client": "^6.2.1",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.6",
        "@typescript-eslint/eslint-plugin": "^8.20.0",
        "@typescript-eslint/parser": "^8.20.0",
        "compression": "^1.7.5",
        "dotenv": "^16.4.7",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.1",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "jest": "^29.7.0",
        "luxon": "^3.5.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "env-var": "^7.5.0"
    }
}
