{
    "info": {
        "_postman_id": "calendar-api",
        "name": "Calendar API",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Create Calendar",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\"name\":\"My Calendar\"}"
                },
                "url": {
                    "raw": "http://localhost:8080/calendars",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8080",
                    "path": [
                        "calendars"
                    ]
                }
            }
        },
        {
            "name": "Create Calendar Entry",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\"calendarId\": \"12345\", \"entry\": {\"title\": \"New Entry\", \"description\": \"This is a new entry\"}}"
                },
                "url": {
                    "raw": "http://localhost:8080/calendars/:calendarId/entries",
                    "host": [
                        "localhost"
                    ],
                    "port": "8080",
                    "path": [
                        "calendars",
                        ":calendarId",
                        "entries"
                    ]
                }
            }
        },
        {
            "name": "Get Calendar Entries",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8080/calendars/:calendarId/entries",
                    "host": [
                        "localhost"
                    ],
                    "port": "8080",
                    "path": [
                        "calendars",
                        ":calendarId",
                        "entries"
                    ]
                }
            }
        },
        {
            "name": "Update Calendar Entry",
            "request": {
                "method": "PUT",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\"calendarId\": \"12345\", \"entryId\": \"67890\", \"entry\": {\"title\": \"Updated Entry\", \"description\": \"This is an updated entry\"}}"
                },
                "url": {
                    "raw": "http://localhost:8080/calendars/:calendarId/entries/:entryId",
                    "host": [
                        "localhost"
                    ],
                    "port": "8080",
                    "path": [
                        "calendars",
                        ":calendarId",
                        "entries",
                        ":entryId"
                    ]
                }
            }
        },
        {
            "name": "Delete Calendar Entry",
            "request": {
                "method": "DELETE",
                "header": [],
                "url": {
                    "raw": "http://localhost:8080/calendars/:calendarId/entries/:entryId",
                    "host": [
                        "localhost"
                    ],
                    "port": "8080",
                    "path": [
                        "calendars",
                        ":calendarId",
                        "entries",
                        ":entryId"
                    ]
                }
            }
        }
    ]
}