{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "moduleNameMapper": {
        "^@components/(.*)$": "<rootDir>/src/components/$1",
        "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    },
    "roots": [
        "<rootDir>/dist"
    ],
    "testEnvironment": "node"
}